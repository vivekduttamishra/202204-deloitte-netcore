{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
    "AllowedHosts": "*",
    "staticfiles": {
        "root": "wwwroot"
    },

    "prefix": "Hello",
    "suffix": ", Welcome to our Service",

    "greeter": {
        "prefix": "Hey ",
        "suffix": ", glad to have you with us today"
    },
    "urlstats": {
        "log404InVisits": true,
        "excludeUrls": ["/stats","/404"]
    }
}
